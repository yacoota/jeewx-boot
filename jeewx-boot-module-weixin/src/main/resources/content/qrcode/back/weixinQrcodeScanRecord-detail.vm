<!DOCTYPE html>
<html lang="en">
#parse("content/base/back/common/head.vm")
<body style='overflow:scroll;overflow-x:hidden'>
	<div class="container bs-docs-container" style="width:100%;">
		<div class="row">
			<div class="panel panel-default">
				<div class="panel-heading">
				<button type="button" class="btn btn-default" id="formReturn" data-dismiss="modal" onclick="doUrl('$!{basePath}/qrcode/back/weixinQrcodeScanRecord/list.do')"><span class="glyphicon glyphicon-circle-arrow-left"></span> 返回</button>
				</div>
				  	<div class="panel-body">
				   		<form class="form-horizontal" role="form">
							  <fieldset disabled>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">openid</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{weixinQrcodeScanRecord.openid}" name="openid" id="openid" class="form-control" />
													    </div>
													  </div>
													  <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">扫码时间</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{weixinQrcodeScanRecord.scanTime})" name="scanTime" id="scanTime" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">场景值ID</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{weixinQrcodeScanRecord.sceneId}" name="sceneId" id="sceneId" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">公众号ID</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{weixinQrcodeScanRecord.jwid}" name="jwid" id="jwid" class="form-control" />
													    </div>
													  </div>
													 <div class="form-group mno">
													    <label for="inputEmail3" class="col-sm-1 control-label" style="text-align:left;">是否扫码关注 0:非扫码关注,1:扫码关注</label>
													    <div class="col-sm-2">
													      <input type="text" value="$!{weixinQrcodeScanRecord.isScanSubscribe}" name="isScanSubscribe" id="isScanSubscribe" class="form-control" />
													    </div>
													  </div>
							  </fieldset>
						</form>
					</div>
			</div>
		</div>
	</div>
</body>
</html>